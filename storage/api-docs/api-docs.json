{
    "openapi": "3.0.0",
    "info": {
        "title": "API APR",
        "description": "Documentación de la API del sistema de Agua Potable Rural",
        "version": "1.0.0"
    },
    "paths": {
        "/api/login": {
            "post": {
                "tags": [
                    "Autenticación"
                ],
                "summary": "Iniciar sesión",
                "description": "Permite iniciar sesión con email y contraseña. Devuelve un token Bearer para autenticación.",
                "operationId": "acbd311a11e233cda27fe89ca59eb4ca",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "luciano@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "12345678"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Inicio de sesión exitoso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "user": {
                                            "type": "object"
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "1|eyJ0eXAiOiJKV..."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Credenciales incorrectas"
                    }
                }
            }
        },
        "/api/register": {
            "post": {
                "tags": [
                    "Autenticación"
                ],
                "summary": "Registrar nuevo usuario",
                "description": "Crea un nuevo usuario y devuelve su token de acceso.",
                "operationId": "34ea508192444f246459bedb9264e205",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "run",
                                    "nombre_usuario",
                                    "nombre",
                                    "apellido",
                                    "email",
                                    "password",
                                    "password_confirmation"
                                ],
                                "properties": {
                                    "run": {
                                        "type": "string",
                                        "example": "12345678-9"
                                    },
                                    "nombre_usuario": {
                                        "type": "string",
                                        "example": "luciano.dev"
                                    },
                                    "nombre": {
                                        "type": "string",
                                        "example": "Luciano"
                                    },
                                    "apellido": {
                                        "type": "string",
                                        "example": "Andrade"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "luciano@example.com"
                                    },
                                    "telefono_movil": {
                                        "type": "string",
                                        "example": "+56912345678"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "12345678"
                                    },
                                    "password_confirmation": {
                                        "type": "string",
                                        "example": "12345678"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Usuario registrado exitosamente",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "user": {
                                            "type": "object"
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "1|abc123..."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Error de validación"
                    }
                }
            }
        },
        "/api/logout": {
            "post": {
                "tags": [
                    "Autenticación"
                ],
                "summary": "Cerrar sesión",
                "description": "Cierra la sesión actual eliminando el token.",
                "operationId": "4adbe978658f0abeac7aaaa21380a79c",
                "responses": {
                    "200": {
                        "description": "Sesión cerrada correctamente"
                    },
                    "401": {
                        "description": "No autenticado"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/me": {
            "get": {
                "tags": [
                    "Autenticación"
                ],
                "summary": "Obtener usuario autenticado",
                "description": "Devuelve los datos del usuario autenticado usando Sanctum.",
                "operationId": "e7e8b4580b88a5127e0812a8b2519a59",
                "responses": {
                    "200": {
                        "description": "Usuario autenticado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "id": {
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "nombre": {
                                            "type": "string",
                                            "example": "Luciano"
                                        },
                                        "apellido": {
                                            "type": "string",
                                            "example": "Andrade"
                                        },
                                        "email": {
                                            "type": "string",
                                            "example": "luciano@example.com"
                                        },
                                        "telefono_movil": {
                                            "type": "string",
                                            "example": "+56912345678"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "No autenticado"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/usuarios": {
            "get": {
                "tags": [
                    "Usuarios"
                ],
                "summary": "Listar todos los usuarios",
                "description": "Devuelve una lista de todos los usuarios activos.",
                "operationId": "af2f4bbfef81f44540e247894aa83542",
                "responses": {
                    "200": {
                        "description": "Lista de usuarios",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "Usuarios"
                ],
                "summary": "Crear un nuevo usuario",
                "description": "Crea un nuevo usuario en el sistema.",
                "operationId": "7540001b72df77ace2e577c780c90944",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "nombre",
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "run": {
                                        "type": "string",
                                        "example": "12345678-9"
                                    },
                                    "nombre_usuario": {
                                        "type": "string",
                                        "example": "lAndrade"
                                    },
                                    "telefono_movil": {
                                        "type": "string",
                                        "example": "912345678"
                                    },
                                    "nombre": {
                                        "type": "string",
                                        "example": "Luciano"
                                    },
                                    "apellido": {
                                        "type": "string",
                                        "example": "Andrade"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "luciano@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "12345678"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Usuario creado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Usuario creado"
                                        },
                                        "data": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Error de validación"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/usuarios/{id}": {
            "get": {
                "tags": [
                    "Usuarios"
                ],
                "summary": "Obtener un usuario por ID",
                "description": "Devuelve los datos de un usuario específico.",
                "operationId": "170c3390dbfcea63a77d9a9ae08c3ceb",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Datos del usuario",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Usuario no encontrado"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            },
            "put": {
                "tags": [
                    "Usuarios"
                ],
                "summary": "Actualizar usuario",
                "description": "Modifica los datos de un usuario.",
                "operationId": "8f5986c32553bd1e1a59ecc88134baa5",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "run": {
                                        "type": "string",
                                        "example": "12345678-9"
                                    },
                                    "nombre_usuario": {
                                        "type": "string",
                                        "example": "lAndrade"
                                    },
                                    "telefono_movil": {
                                        "type": "string",
                                        "example": "912345678"
                                    },
                                    "nombre": {
                                        "type": "string",
                                        "example": "Luciano"
                                    },
                                    "apellido": {
                                        "type": "string",
                                        "example": "Andrade"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "luciano@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "12345678"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Usuario actualizado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Usuario no encontrado"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            },
            "delete": {
                "tags": [
                    "Usuarios"
                ],
                "summary": "Desactivar (soft delete) un usuario",
                "description": "Marca un usuario como inactivo en lugar de eliminarlo completamente.",
                "operationId": "b98f1a7d5edcdd2a09b7655dfd070d89",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Usuario desactivado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Usuario desactivado"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Usuario no encontrado"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/usuarios/{id}/restaurar": {
            "put": {
                "tags": [
                    "Usuarios"
                ],
                "summary": "Restaurar usuario eliminado",
                "description": "Restaura un usuario previamente eliminado (SoftDeletes).",
                "operationId": "5a0adc02a9272bae7946f01f507e6293",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Usuario restaurado correctamente",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Usuario restaurado correctamente"
                                        },
                                        "data": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Usuario no encontrado"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/usuarios-eliminados": {
            "get": {
                "tags": [
                    "Usuarios"
                ],
                "summary": "Listar usuarios eliminados",
                "description": "Devuelve los usuarios que han sido eliminados (SoftDelete).",
                "operationId": "9f5d789ecfbce481342eda0fd7d128f3",
                "responses": {
                    "200": {
                        "description": "Lista de usuarios eliminados",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "sanctum": {
                "type": "http",
